{
    "name": "dytechltd/nova-permission",
    "description": "A Laravel Nova tool for Spatie's Permission library.",
    "keywords": [
        "laravel",
        "nova",
        "spatie-permission"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "spatie/laravel-permission": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Vyuldashev\\NovaPermission\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vyuldashev\\NovaPermission\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [{
        "type": "package",
	"version": "2.0.0", 
            "source": { 
            	"url": "https://github.com/dytechltd/nova-permission.git",
                "type": "git", 
                "reference": "master" 
         }
    }]
}
